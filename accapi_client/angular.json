{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "accwebagent": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "./dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/sitemap.xml",
              "src/googled41787c6aae2151b.html",
              "src/CNAME",
              "src/start.html",
              "src/acc_crmClicktoCall.html",
              "src/manifest.json"
            ],
            "styles": [
              "node_modules/primeng/resources/themes/nova/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeicons/primeicons.css",
              "src/assets/css/reset.css",
              "src/assets/css/loading.css",
              "src/app/styles.scss"
            ],
            "scripts": [
              "src/assets/js/modernizr.js",
              "src/assets/js/gtm.js"
            ],
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": false,
            "optimization": true,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "outputHashing": "all",
              "serviceWorker": true,
              "ngswConfigPath": "src/ngsw-config.json",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "accwebagent:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "accwebagent:build:production"
            },
            "development": {
              "browserTarget": "accwebagent:build:development"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "accwebagent:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "src/assets/js/modernizr.js",
              "src/assets/js/gtm.js"
            ],
            "styles": [
              "src/assets/css/reset.css",
              "src/assets/css/loading.css",
              "src/assets/css/styles.scss",
              "src/scss/styles.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/sitemap.xml",
              "src/googled41787c6aae2151b.html",
              "src/CNAME",
			  "src/start.html"
            ]
          }
        }
      }
    },
    "accwebagent-e2e": {
      "root": "",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "acc-agent-api:serve"
          }
        }
      }
    }
  },
  "defaultProject": "accwebagent",
  "schematics": {
  },
  "cli": {
    "analytics": false
  }
}
